  "scripts": {
    "start": "babel-node tools/run.ts start --extensions '.ts,.tsx'",
    "debug": "babel src --out-dir debug --extensions '.ts,.tsx'",
    "build": "babel-node tools/run.ts build --extensions '.ts,.tsx'",
    "build:stats": "babel-node tools/run.ts build --extensions '.ts,.tsx' --release",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular.io-example:server",
    "clean": "babel-node tools/run.ts clean --extensions '.ts,.tsx' ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tools-debug": "babel tools --out-dir debug --extensions '.ts,.tsx'",
    "dist": "babel tools --extensions '.ts,.tsx' --out-dir dist"
  },
  "repository": {
    "url": "https://github.com/HWuYing/ts-tools",
    "type": "git"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-transform-typescript",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ],
    "sourceMaps": "inline",
    "retainLines": true
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ]
}
